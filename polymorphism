#include <iostream>
using namespace std;
class Example {
   public :
   void func(int a) {
      cout << "
The value of a: " << a;
   }
   void func(int a, int b) {
      cout << "
The value of a: " << a;
      cout << "
The value of b: " << b;
   }
   void func(char c) {
      cout << "
The value of c: " << c;
   }
};
int main() {
   Example obj;
   cout<< "
One int value";
   obj.func(5);
   cout<< "
One char value";
   obj.func('A');
   cout<< "
Two int values";
   obj.func(7, 2);
   return 0;
}
